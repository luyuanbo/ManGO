<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--个人商品、店铺、足迹-->

    <style name="AppF4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15dp</item>
    </style>

    <!--个人中布局前面的小图片-->
    <style name="AppF4d_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:paddingLeft">15dp</item>
    </style>
    <!--个人中布局-->
    <style name="AppF4d_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <!--个人应用布局-->
    <style name="AppF4d_layouts">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <!--个人字体布局-->
    <style name="AppF4d_zi">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <!--小应用布局局-->
    <style name="AppF4d_Xyy">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">6dp</item>
    </style>

    <style name="App_Register">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:ems">12</item>
    </style>

    <style name="home_classfiled_img">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">70dp</item>
    </style>


    <style name="home_classifiled_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:text">@string/home_classifiled</item>
    </style>


</resources>
